<div class="col-xs-10 col-xs-offset-1 homePage">
  <h1 id="welcomeBanner">Welcome to Hair Etc...</h1>
  <% if !signed_in? %>
  
    <h6 class="text-center">What would you like to do?</h6>
    <%= button_to "Sign Up!", new_client_registration_path, method: 'get', class: "btn btn-block btn-success button" %> <br>
    <%= button_to "Sign In!", new_client_session_path, method: 'get', class: "btn btn-block btn-primary button" %>

  <% else %>

    <%= render 'signedin' %>

  <% end %>




</div>  <!-- WelcomePageDiv -->