<h1 class="text-center"><%= current_client.first_name %> <%= current_client.last_name %>'s Appointments</h1>
<p>Find me in app/views/appointments/index.html.erb</p>

<% @appointments.each do |app| %>

<div> <%= app.appt_date_time %>
  <br> <%= app.stylist.first_name %>
    <ul> <% app.offerings.each do |x| %>
      <li> <%= x.name %> </li>
    <% end %>
    </ul>
  <%= link_to "Edit", edit_client_appointment_path(current_client.id, app.id) %>
</div>

<% end %>

<%= link_to "New Appointment", new_client_appointment_path(current_client.id) %>


<div class="xs-col-4">


</div>  <!-- End of repeated div -->